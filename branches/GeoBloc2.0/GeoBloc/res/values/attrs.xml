<?xml version="1.0" encoding="utf-8"?>
<resources>
	<declare-styleable name="ThreeStateCheckBox">
	    <attr name="label" format="string" />
	    <attr name="shadow" format="boolean" />
	    <attr name="state" format="integer" />
	</declare-styleable>
	
	<declare-styleable name="ProgressItem">
	    <attr name="progress_label" format="string" />
	    <attr name="bar_visible" format="boolean" />
	</declare-styleable>
	
	<declare-styleable name="GalleryPhoto">
        <attr name="android:galleryItemBackground" />
    </declare-styleable>
	
</resources>
